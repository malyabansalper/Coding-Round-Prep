   "source": [
    "# To find the sum of digits in a given Number\n",
    "n=int(input(\"Enter a number : \"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sumOfDigits(num):\n",
    "    add=0\n",
    "    while num>0:\n",
    "        d=num%10\n",
    "        add=add+d\n",
    "        num=num//10\n",
    "    return add"
   ]
  },
