    "# checking for all the possible subarrays\n",
    "def findSubArray(arr):\n",
    "    l=[[]]\n",
    "    for i in range(len(arr)+1):\n",
    "        for j in range(i):\n",
    "            l.append(arr[j:i])\n",
    "    return l"
